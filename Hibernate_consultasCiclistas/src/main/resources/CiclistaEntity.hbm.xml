<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.example.DB.CiclistaEntity" table="Ciclista" schema="ciclistas">
        <id name="dorsal">
            <column name="dorsal" sql-type="int"/>
        </id>
        <property name="edad">
            <column name="edad" sql-type="int" not-null="true"/>
        </property>
        <property name="nombre">
            <column name="nombre" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="nomeq">
            <column name="nomeq" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>